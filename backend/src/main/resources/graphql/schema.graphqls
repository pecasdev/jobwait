type Query {
    userById(id: ID): User
    promptById(id: ID): Prompt
}

type User {
    id: ID!
    prompt: [Prompt!]!
}

type Prompt {
    id: ID!
    question: String!
    response: Response
}

type IntResponse {
    intResponse: Int
}

type StrResponse {
    strResponse: String
}

union Response = IntResponse | StrResponse
